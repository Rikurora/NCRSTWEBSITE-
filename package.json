{
  "name": "ncrst-website",
  "private": true,
  "version": "1.0.0",
  "description": "NCRST Website Monorepo",
  "scripts": {
    "dev:cms": "pnpm --filter cms dev",
    "dev:website": "pnpm --filter website dev",
    "dev:backend": "pnpm --filter backend dev",
    "build:cms": "pnpm --filter cms build",
    "build:website": "pnpm --filter website build",
    "build:backend": "pnpm --filter backend build",
    "dev": "pnpm --parallel dev:*",
    "build": "pnpm build:backend && pnpm build:cms && pnpm build:website",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "devDependencies": {
    "prettier": "^3.2.0"
  }
} 